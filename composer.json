{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "intervention/image": "^2.3",
        "laracasts/flash": "^2.0",
        "doctrine/dbal": "^2.5",
        "yajra/laravel-datatables-oracle": "~6.0",
        "zizaco/entrust": "5.2.x-dev",
        "milon/barcode": "^5.3",
        "barryvdh/laravel-dompdf": "~0.8.0",
        "maatwebsite/excel": "~2.1.0",
        "mpdf/mpdf": "^6.1",
        "nesbot/carbon": "^1.38",
        "guzzlehttp/guzzle": "^6.5",
        "arcanedev/log-viewer": "~4.2.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
        "app/Helpers/CodCalculatorHelper.php", 
        "app/Helpers/MerchantSubOrderHelper.php" ,
        "app/Helpers/HubSubOrderHelper.php" ,
        "app/Helpers/ReconciliationHelper.php",
        "app/Helpers/TatHelper.php",
        "app/Helpers/AgingHelper.php",
        "app/Helpers/iPostStatus.php",
        "app/Helpers/helpers.php",
        "app/Helpers/fastBazzar.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
